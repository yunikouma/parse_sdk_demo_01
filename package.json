    "intersect": "1.0.1",
    "lodash": "4.17.4",
    "lru-cache": "4.1.1",
    "mime": "2.2.0",
    "mongodb": "3.0.1",
    "multer": "1.3.0",
    "parse": "1.11.0",
    "pg-promise": "7.4.0",
    "redis": "2.8.0",
